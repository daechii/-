        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_4333Project"
        mc:Ignorable="d"
        Title="_4333_Pavlova" Height="450" Width="800">
    <Grid>
        
            <StackPanel VerticalAlignment="Center">
            <TextBlock HorizontalAlignment="Center" FontSize="30">Павлова Дарья</TextBlock>
            <TextBlock HorizontalAlignment="Center" FontSize="30">19 лет</TextBlock>
            <Button Width="150" Height="20" Content="Импорт данных из Excel" Click="Button_Click" x:Name="Import"/>
